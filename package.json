{
  "name": "ms-orders-management",
  "version": "1.0.0",
  "description": "Orders mangagement microservice for ISDIN",
  "author": "Aleix Jim√©nez Ferrer <aleixjf@gmail.com>",
  "license": "ISC",
  "private": true,
  "engines": {
    "node": ">=22.0.0 <23.0.0",
    "npm": ">=11.0.0 <12.0.0"
  },
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build": "nest build",
    "serve:prod": "NODE_ENV=production node dist/src/main",
    "docker:dev": "docker compose -f docker-compose.yml -f docker/development.compose.yml up",
    "docker:export": "docker save isdin-microservice -o docker/images/isdin-microservice.tar",
    "docker:prod:build": "docker compose -f docker-compose.yml -f docker/production.compose.yml build --no-cache",
    "docker:prod:run": "docker compose -f docker-compose.yml -f docker/production.compose.yml up",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.config.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --check \"{src,test}/**/*.ts\"",
    "format:fix": "prettier --write \"{src,test}/**/*.ts\"",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d database/typeorm.config.ts",
    "migration:generate": "npm run typeorm migration:generate -- database/migrations/migration",
    "migration:list": "npm run typeorm migration:show",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "typeorm-extension": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/bin/cli.cjs -d database/typeorm.config.ts",
    "db:create": "npm run typeorm-extension db:create",
    "db:drop": "npm run typeorm-extension db:drop",
    "db:seed": "npm run typeorm-extension seed:run",
    "db:setup": "npm run db:create && npm run typeorm-extension seed:run -- --name=setup.ts",
    "db:reset": "npm run db:drop && npm run db:setup",
    "db:schema": "rimraf database/migrations/*.ts && npm run db:drop && npm run db:create && npm run migration:generate && npm run migration:run"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.15",
    "@grpc/reflection": "^1.0.4",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/microservices": "^11.1.3",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/uuid": "^10.0.0",
    "@web-std/file": "^3.0.3",
    "axios": "^1.10.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.0",
    "crypto": "^1.0.1",
    "helmet": "^8.1.0",
    "kafkajs": "^2.2.4",
    "pg": "^8.16.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.25",
    "typeorm-extension": "^3.7.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.3",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/jest": "30.0.0",
    "@types/node": "24.0.4",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "30.0.3",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.8.3"
  },
  "volta": {
    "node": "22.17.0",
    "npm": "11.4.2"
  }
}
